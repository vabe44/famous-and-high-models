<html>

<head>
    <meta charset="UTF-8">
    <title>Models - Famous & High Models</title>
    <link rel="stylesheet" type="text/css" href="/css/paper.min.css" />
    <link rel="stylesheet" type="text/css" href="/css/semantic.min.css" />
    <link rel="stylesheet" type="text/css" href="/css/icon.min.css" />
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <link rel="stylesheet" type="text/css" href="/css/slick.css" />
    <script type="text/javascript" src="/js/jquery-2.2.4.min.js"></script>
    <script type="text/javascript" src="/js/semantic.min.js"></script>

    <style>
    #header {
        height: 26.5%;
        padding-left: 18px;
    }
    </style>

</head>

<body>

    <% include ../partials/header %>

    <div id="container">

        <div id="modelsgrid">
            <% models.forEach(function(model){ %>

                <div class="modelbox">
                    <a href="/models/<%= model.id %>">
                        <div class="modelimage">
                            <img class="no-border" src="<%= model.Images[0].link %>" alt="<%= model.Images[0].title %>">
                        </div>
                        <div class="modelname">
                            <h5>
                                <%= model.get('fullName') %>
                            </h5>
                        </div>
                    </a>
                </div>

            <% }); %>
        </div>

    </div>

    <script>
        $('.ui.dropdown').dropdown();
    </script>

</body>

</html>